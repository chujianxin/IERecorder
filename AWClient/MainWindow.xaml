        <!-- Other-Style  -->
        <Style TargetType="ListBoxItem" x:Key="Other">
            <!--<EventSetter Event="MouseDoubleClick"  Handler="ListBoxMouseDouleClick">
            </EventSetter>-->
            <Setter Property="SnapsToDevicePixels" Value="true"/>
            <Setter Property="OverridesDefaultStyle" Value="true"/>
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="ListBoxItem">
                        <Border   
                       Name="Border"                       
                       Padding="2"    
                        SnapsToDevicePixels="true" Margin="0,0,0,0">

                            <Grid>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="30"/>
                                    <ColumnDefinition Width="10"/>
                                    <ColumnDefinition/>
                                </Grid.ColumnDefinitions>
                                <Image Width="30" Height="30" Source="ListItemImg/Other.png" Grid.Column="0"/>

                                <ContentPresenter Grid.Column="2" VerticalAlignment="Center" ></ContentPresenter>
                            </Grid>
                        </Border>
                        <ControlTemplate.Triggers>
                            
                            <Trigger Property="IsSelected" Value="false">
                                <Setter TargetName="Border" Property="Background"  
                    Value="#212328"/>
                                <Setter Property="Foreground"  
                    Value="#94a0b4"/>
                            </Trigger>
                            <Trigger Property="IsSelected" Value="true">
                                <Setter TargetName="Border" Property="Background"  
                    Value="#3a8ed6"/>
                                <Setter Property="Foreground"  
                    Value="White"/>
                            </Trigger>

                        </ControlTemplate.Triggers>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style>

