                                </Grid.ColumnDefinitions>
                                <TextBlock Text="ID" FontSize="15" Grid.Column="0" Foreground="#212328" Margin="75,2,5,0" />
                                <TextBox x:Name="txtID"  Grid.Column="1" Padding="3"  Width="168" FontSize="12" Margin="0,0,78,0"></TextBox>
                            </Grid>
                            <Grid Grid.Row="3">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="130"></ColumnDefinition>
                                    <ColumnDefinition></ColumnDefinition>
                                </Grid.ColumnDefinitions>
                                <TextBlock Text="Name" FontSize="15" Grid.Column="0" Foreground="#212328" Margin="75,2,5,0" />
                                <TextBox x:Name="txtName"  Grid.Column="1" Padding="3"  Width="168" FontSize="12" Margin="0,0,78,0"></TextBox>
                            </Grid>
                            <Grid Grid.Row="5">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="130"></ColumnDefinition>
                                    <ColumnDefinition></ColumnDefinition>
                                </Grid.ColumnDefinitions>
                                <TextBlock Text="Action" FontSize="15" Grid.Column="0" Foreground="#212328" Margin="75,2,5,0" />
                                <ComboBox x:Name="cboType" VerticalContentAlignment="Center" Grid.Column="1" Width="168"  Padding="0" Margin="0,0,78,0">
                                    <ComboBoxItem Content="CLICK" ></ComboBoxItem>
                                    <ComboBoxItem Content="INPUT" ></ComboBoxItem>
                                    <ComboBoxItem Content="HOVER" ></ComboBoxItem>
                                    <ComboBoxItem Content="DOUBLECLICK" ></ComboBoxItem>
                                    <ComboBoxItem Content="RIGHTCLICK" ></ComboBoxItem>
                                    <ComboBoxItem Content="SELECT" ></ComboBoxItem>
                                    <ComboBoxItem Content="MOUSEMOVE" ></ComboBoxItem>
                                    <ComboBoxItem Content="MOUSEDOWN" ></ComboBoxItem>
                                    <ComboBoxItem Content="MOUSEOVER" ></ComboBoxItem>
                                    <ComboBoxItem Content="MOUSEOUT" ></ComboBoxItem>
                                    <ComboBoxItem Content="MOUSEUP" ></ComboBoxItem>
                                    <ComboBoxItem Content="KEYPRESS" ></ComboBoxItem>
                                </ComboBox>
                            </Grid>

                            <Grid Grid.Row="7">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="130"></ColumnDefinition>
                                    <ColumnDefinition></ColumnDefinition>
                                </Grid.ColumnDefinitions>
                                <TextBlock Text="Value" FontSize="15"  Grid.Column="0" Foreground="#212328" Margin="81,2,5,0" />
                                <TextBox x:Name="txtValue"  Grid.Column="1" Padding="3"  Width="168" FontSize="12" Margin="0,0,78,0"></TextBox>
                            </Grid>

                            <Grid Grid.Row="9">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="130"></ColumnDefinition>
                                    <ColumnDefinition></ColumnDefinition>
                                </Grid.ColumnDefinitions>
                                <TextBlock Text="Comments" FontSize="15"  Grid.Column="0" Foreground="#212328" Margin="40,2,5,0" />
                                <TextBox x:Name="txtComments" Grid.Column="1" Width="168"  Padding="3" FontSize="12" Margin="0,0,78,0"></TextBox>
                            </Grid>

                            <Button  Content="保存"  Width="220" Grid.Row="11" Foreground="White" FontSize="11" BorderBrush="{x:Null}" Style="{DynamicResource Fold-AW}" Click="OnSaveAWStep" >
                                <Button.Background>
                                    <ImageBrush ImageSource="img/Fold_save1.jpg"/>
                                </Button.Background>
                            </Button>

                        </Grid>

                    </Border>


                    <Border CornerRadius="3"  Background="#d6dff2" Grid.Row="3" HorizontalAlignment="Left" Width="376"  >

                        <Grid>

                            <Grid.RowDefinitions>
                                <RowDefinition Height="50"></RowDefinition>
                                <RowDefinition Height="170"></RowDefinition>
                                <RowDefinition Height="10"></RowDefinition>
                                <RowDefinition Height="30"></RowDefinition>
                                <RowDefinition></RowDefinition>
                            </Grid.RowDefinitions>

                            <TextBlock Text="Map" Grid.Row="0" Foreground="#212328" FontSize="17" Margin="15 20 0 0"/>

                            <Border CornerRadius="3" Grid.Row="1" Width="270" Background="#FFD6DFF2">

                                <ListBox x:Name="listbox" Background="#FFD6DFF2" Margin="2 2 2 2" Style="{DynamicResource ListBoxStyle2}">
                                </ListBox>

                            </Border>
                            <Button  Content="编辑"  Width="220" Grid.Row="3" Foreground="White" FontSize="11" BorderBrush="{x:Null}" Style="{DynamicResource Fold-Map}" Click="Button_Click">
                                <Button.Background>
                                    <ImageBrush ImageSource="img/Fold_save-1.jpg"/>
                                </Button.Background>
                            </Button>

                        </Grid>
                    </Border>
                </Grid>
            </Grid>
        </Grid>
    </Grid>
</Window>
